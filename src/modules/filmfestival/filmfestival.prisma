model FilmFestival {​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​​
    id                 Int        @id @default(autoincrement())
    filmFestivalName   String?    @map("filmFestivalName")
    address            String?    @map("address")
    chairman_Headedby  String?    @map("chairman_Headedby")
    aboutUs            String?    @map("aboutUs")
    startDate          DateTime?   @default(now()) @db.Date @map("startDate")
    endDate            DateTime?   @default(now()) @db.Date @map("endDate")
    movieTittle        String?    @map("movieTittle")
    movieType          String?    @map("movieType")
    briefSynopsis      String?    @map("briefSynopsis")
    movieSpecificationMovieType          String?    @map("movieSpecificationMovieType")
    genres             String?   @map("genres")
    runTime            DateTime?  @db.Time @map("runTime")
    completedDate      String?    @map("completedDate")
    productionBudget   String?    @map("productionBudget")
    countryOfOrigin    String?    @map("countryOfOrigin")
    state              String?    @map("state")
    language           String?    @map("language")
    value              Json[]     @map("value")
    userId             Int        @map("user_id")
}