model Form {
   id             Int      @id @default(autoincrement())
   createdAt      DateTime @default(now()) @map("created_at")
   updatedAt      DateTime   @default(now()) @map("updated_at")
   createdBy      String?     @map("created_by")
   updatedBy      String?    @map("updated_by")
   name           String? @map("name")
   status          String? @map("status")
}

model FormElements {
   id             Int        @id @default(autoincrement())
   createdAt      DateTime   @default(now()) @map("created_at")
   updatedAt      DateTime   @default(now()) @map("updated_at")
   createdBy      String?     @map("created_by")
   updatedBy      String?    @map("updated_by")
   element        String?    @map("element")
   text           String?    @map("text")
   static         String?    @map("static")
   required       String?    @map("required")
   bold           String?    @map("bold")
   italic         String?    @map("italic")
   content        String?    @map("content")
   canHavePageBreakBefore           String?    @map("can_have_page_break_before")
   canHaveAlternateForm             String?    @map("can_have_alternate_form")
   canHaveDisplayHorizontal         String?    @map("can_have_display_horizontal")
   canHaveOptionCorrect             String?    @map("can_have_option_correct")
   canHaveOptionValue               String?    @map("can_have_option_value")
   canPopulateFromApi               String?    @map("can_populate_fromApi")
   canHaveAnswer                    String?    @map("can_have_answer")
   fieldName                        String?    @map("field_name")
   label                            String?    @map("label")
   formId         Int        @map("form_id")
   form           Form       @relation(fields: [formId], references: [id])
}

model FormOptions{
 id                    Int        @id @default(autoincrement())
createdAt             DateTime   @default(now()) @map("created_at")
updatedAt             DateTime   @default(now()) @map("updated_at")
createdBy             String?     @map("created_by")
updatedBy              String?    @map("updated_by")
 value                String?     @map("value")
 text                 String?     @map("text")
 key                  String?     @map("key") 
 correct              String?     @map("correct") 
 formElementsId       Int         @map("form_elements_id")
 Formelements         FormElements       @relation(fields: [formElementsId], references: [id])
}